<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jm.springboottemplate.common.dao.TestTableDao">
    <select id="getById" parameterType="com.jm.springboottemplate.common.domain.TestTable"
            resultType="com.jm.springboottemplate.common.domain.TestTable">
        SELECT *
        FROM test_table tt
        WHERE tt.id = #{id}
    </select>
</mapper>